<p id="notice"><%= notice %></p>

<h1>Filesets</h1>

<table class="table">
  <thead>
    <tr>
      <th>Dataset</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @filesets.each do |fileset| %>
      <tr>
        <td><%= link_to fileset.dataset_id, fileset.dataset %></td>
        <td><%= link_to 'Show', fileset %></td>
        <td><%= link_to 'Edit', edit_fileset_path(fileset) %></td>
        <td><%= link_to 'Destroy', fileset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate(@filesets) %>
