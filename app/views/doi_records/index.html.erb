<p id="notice"><%= notice %></p>

<h1>Doi Records</h1>

<table>
  <thead>
    <tr>
      <th>Identifier</th>
      <th>Dataset</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doi_records.each do |doi_record| %>
      <tr>
        <td><%= doi_record.identifier %></td>
        <td><%= doi_record.dataset_id %></td>
        <td><%= link_to 'Show', doi_record %></td>
        <td><%= link_to 'Edit', edit_doi_record_path(doi_record) %></td>
        <td><%= link_to 'Destroy', doi_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doi Record', new_doi_record_path %>
